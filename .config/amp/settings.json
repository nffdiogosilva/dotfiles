{
  "amp.permissions": [],
  "amp.mcpServers": {
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_MCP_TOKEN}"
      }
    },
    "slack": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-slack"
      ],
      "env": {
        "SLACK_BOT_TOKEN": "${SLACK_BOT_TOKEN}",
        "SLACK_TEAM_ID": "TMHDNUL3W"
      }
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ]
    },
    "figma-dev-mode-mcp-server": {
      "url": "http://127.0.0.1:3845/sse"
    }
  },
  // "amp.experimental.modes": [
  //   "bombadil"
  // ],
  "amp.mcpTrustedServers": [
    {
      "serverName": "atlassian",
      "specHash": "28f30dd29979dfd05bb8e632f91c20b24d353ba5fcb29b63e9592b1307327ff6",
      "allow": true
    }
  ],
  "amp.dangerouslyAllowAll": true,
  "amp.terminal.theme": "readable-diff"
}
